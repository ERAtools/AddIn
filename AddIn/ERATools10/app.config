<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="ERAtools10.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog" type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL" initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <ERAtools10.My.MySettings>
            <setting name="LastFormat" serializeAs="String">
                <value />
            </setting>
            <setting name="LastBufferSize" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="LastBufferUnit" serializeAs="String">
                <value>Miles</value>
            </setting>
            <setting name="LastOutput" serializeAs="String">
                <value />
            </setting>
            <setting name="LastSource" serializeAs="String">
                <value />
            </setting>
            <setting name="OutputFormatFolder" serializeAs="String">
                <value />
            </setting>
            <setting name="ScratchWorkspace" serializeAs="String">
                <value />
            </setting>
            <setting name="AuthorName" serializeAs="String">
                <value />
            </setting>
            <setting name="MaxReportsLimit" serializeAs="String">
                <value>25</value>
            </setting>
            <setting name="MaxUnionedShapesLimit" serializeAs="String">
                <value>1000</value>
            </setting>
            <setting name="WarnUnionedShapesLimit" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="WarnReportsLimit" serializeAs="String">
                <value>10</value>
            </setting>
            <setting name="WarnReplaceOutput" serializeAs="String">
                <value>True</value>
            </setting>
        </ERAtools10.My.MySettings>
    </userSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
</configuration>
